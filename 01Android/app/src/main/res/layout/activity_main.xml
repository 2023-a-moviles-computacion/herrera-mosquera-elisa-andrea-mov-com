<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2196F3"
    android:backgroundTint="#F6F6F6"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_Ciclo_Vida"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ciclo Vida"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_ir_list_view"
        app:layout_constraintVertical_bias="0.271"
        tools:layout_editor_absoluteX="174dp" />

    <Button
        android:id="@+id/btn_ir_list_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="List View"
        app:layout_constraintEnd_toEndOf="@+id/btn_Ciclo_Vida"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn_Ciclo_Vida"
        tools:layout_editor_absoluteY="563dp" />

    <Button
        android:id="@+id/btn_ir_intent_implicito"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="I. IMPLICITO"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="448dp" />

    <Button
        android:id="@+id/btn_ir_intent_explicito"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="I. EXPLICITO"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="337dp" />

    <Button
        android:id="@+id/btn_reccycle_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="68dp"
        android:text="R. VIEW"
        app:layout_constraintBottom_toBottomOf="@+id/btn_ir_list_view"
        app:layout_constraintEnd_toStartOf="@+id/btn_ir_list_view"
        app:layout_constraintTop_toTopOf="@+id/btn_ir_list_view" />

    <Button
        android:id="@+id/btn_SQlite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="68dp"
        android:text="SQLITE"
        app:layout_constraintBottom_toBottomOf="@+id/btn_Ciclo_Vida"
        app:layout_constraintEnd_toStartOf="@+id/btn_Ciclo_Vida"
        app:layout_constraintTop_toTopOf="@+id/btn_Ciclo_Vida"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btn_google_maps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="48dp"
        android:text="G MAPS"
        app:layout_constraintBottom_toBottomOf="@+id/btn_ir_intent_implicito"
        app:layout_constraintEnd_toStartOf="@+id/btn_ir_intent_implicito"
        app:layout_constraintTop_toTopOf="@+id/btn_ir_intent_implicito"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btn_intent_firebase_ui"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="60dp"
        android:text="LOGIN F UI"
        app:layout_constraintBottom_toTopOf="@+id/btn_google_maps"
        app:layout_constraintEnd_toEndOf="@+id/btn_google_maps"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="@+id/btn_google_maps" />

</androidx.constraintlayout.widget.ConstraintLayout>